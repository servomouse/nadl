{
    "comments": {
		"lineComment": "#",
		"blockComment": [ "--[[", "]]" ]
	},
    "brackets": [
        ["[", "]"],
        ["(", ")"]
    ],
    "autoClosingPairs": [
        ["[", "]"],
        ["(", ")"]
    ],
    "indentationRules": {
        "increaseIndentPattern": "^\\s*(module|inputs|groups|outputs):\\s*$",
        "decreaseIndentPattern": "^\\s*\\]"
    }
}
